{
  "version": 2,
  "buildCommand": "cd Bananabyte && npm run build",
  "outputDirectory": "Bananabyte/.next",
  "installCommand": "cd Bananabyte && npm install",
  "builds": [
    {
      "src": "Bananabyte/package.json",
      "use": "@vercel/next",
      "config": { 
        "distDir": ".next",
        "buildCommand": "npm run build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/Bananabyte/public/static/$1"
    },
    {
      "src": "/images/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/Bananabyte/public/static/images/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/Bananabyte/$1"
    }
  ]
} 